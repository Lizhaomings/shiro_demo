<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.shiro_demo.mapper.SystemLogMapper">

    <sql id="allField">
            A.id,
            A.user_name,
            A.operation,
            A.type,
            A.time,
            A.method,
            A.params,
            A.ip,
            A.location,
            A.create_time,
            A.status,
    </sql>

    <sql id="search">
        where A.status = 1
    </sql>
</mapper>
